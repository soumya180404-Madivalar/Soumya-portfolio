        "from pyspark.sql import SparkSession\n",
        "from pyspark.sql import Row\n",
        "\n",
        "# Start Spark session\n",
        "spark = SparkSession.builder.appName(\"MovieExample\").getOrCreate()\n",
        "\n",
        "# Create sample data\n",
        "movies = [\n",
        "    Row(MovieName=\"Inception\", Rating=8.8, Genre=\"Sci-Fi\"),\n",
        "    Row(MovieName=\"Titanic\", Rating=7.8, Genre=\"Romance\"),\n",
        "    Row(MovieName=\"Avengers\", Rating=8.4, Genre=\"Action\"),\n",
        "    Row(MovieName=\"Joker\", Rating=8.5, Genre=\"Thriller\"),\n",
        "    Row(MovieName=\"Frozen\", Rating=7.5, Genre=\"Animation\")\n",
        "]\n",
        "\n",
        "# Create DataFrame\n",
        "df = spark.createDataFrame(movies)\n",
        "df.show()"
